<!-- Tập lệnh tính toán và thiết lập độ rộng thanh tiến trình -->
<script>
  // Đặt hàm calculateProgressBarWidth ở đây
  function calculateProgressBarWidth(skillLevel) {
    // Thực hiện tính toán chiều rộng thích hợp của thanh tiến trình dựa trên skillLevel ở đây
    // Ví dụ: Chuyển đổi giá trị skillLevel thành đúng định dạng để sử dụng cho width
    // Trả về kết quả là một chuỗi đơn vị %, ví dụ: "80%"
    return skillLevel;
  }
  document.addEventListener("DOMContentLoaded", function () {
    const progressBars = document.querySelectorAll(".progress-bar");

    progressBars.forEach((bar) => {
      const skillLevel = bar.getAttribute("data-skill-level");
      const width = calculateProgressBarWidth(skillLevel); // Sử dụng hàm tính toán chiều rộng ở đây

      bar.style.width = width;
    });
  });
</script>

<!-- PHẦN ADD SKILL -->
<!-- Nút "THÊM SKILL" -->
<button id="addSkillBtn">THÊM SKILL</button>
<!-- Modal/Box thêm kỹ năng -->
<div id="addSkillModal" style="display: none; position: fixed; top: 50%; left: 50%; transform: translate(-50%, -50%); width: 300px; padding: 20px; background-color: #fff; border-radius: 5px; box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);">
  <form id="addSkillForm">
    <input type="text" id="skillNameInput" placeholder="Skill Name" required>
    <input type="text" id="skillLevelInput" placeholder="Skill Level" required>
    <button type="submit">Thêm</button>
    <button id="cancelAddSkillBtn" type="button">HỦY BỎ</button>
  </form>
</div>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    const addSkillBtn = document.getElementById("addSkillBtn");// Lấy tham chiếu đến nút "THÊM KỸ NĂNG"
    const addSkillModal = document.getElementById("addSkillModal"); // Lấy tham chiếu đến cửa sổ modal
    const cancelAddSkillBtn = document.getElementById("cancelAddSkillBtn");// Lấy tham chiếu đến nút "HỦY BỎ"

    addSkillBtn.addEventListener("click", function () {
      addSkillModal.style.display = "block";// Hiển thị cửa sổ modal khi người dùng click vào nút "THÊM KỸ NĂNG"
    });

    cancelAddSkillBtn.addEventListener("click", function () {
      addSkillModal.style.display = "none";// Ẩn cửa sổ modal khi người dùng click vào nút "HỦY BỎ"
    });

    const addSkillForm = document.getElementById("addSkillForm");// Lấy tham chiếu đến form "THÊM KỸ NĂNG"

    addSkillForm.addEventListener("submit", async function (event) {
      event.preventDefault();// Ngăn chặn hành động mặc định của form khi người dùng submit

      const skillName = document.getElementById("skillNameInput").value;// Lấy giá trị từ input "Skill Name"
      const skillLevel = document.getElementById("skillLevelInput").value;// Lấy giá trị từ input "Skill Level"

      try {
        const response = await fetch('/addSkill', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({ skillName, skillLevel })// Gửi dữ liệu kỹ năng mới tới máy chủ thông qua phương thức POST
        });

        if (response.ok) {
          addSkillForm.reset();// Đặt lại form sau khi gửi thành công
          addSkillModal.style.display = "none";// Ẩn cửa sổ modal sau khi gửi thành công
          location.reload(); // Làm mới trang để cập nhật giao diện
        }
      } catch (error) {
        console.error('Error adding skill:', error);// Log lỗi nếu có lỗi xảy ra khi gửi dữ liệu
      }
    });
  });
</script>
<!-- JavaScript sử dụng các sự kiện chuột để cho phép modal được kéo đi. -->
<script>
  // Biến isDragging xác định trạng thái kéo
  let isDragging = false;
  // Các biến lưu trữ tọa độ chuột ban đầu và hiện tại khi thực hiện kéo
  let initialX;
  let initialY;
  let currentX;
  let currentY;
// Lấy cửa sổ modal để thực hiện kéo và thả
  const addSkillModal = document.getElementById('addSkillModal');
// Xử lý sự kiện khi chuột được nhấn xuống trên cửa sổ modal
  addSkillModal.addEventListener('mousedown', function(e) {
      // Thiết lập trạng thái kéo là true
    isDragging = true;
      // Lưu tọa độ chuột khi bắt đầu kéo
    initialX = e.clientX;
    initialY = e.clientY;
    // Đổi con trỏ chuột sang dạng "grabbing" khi đang kéo
    addSkillModal.style.cursor = 'grabbing';
  });
// Xử lý sự kiện khi di chuyển chuột trên toàn bộ tài liệu
  document.addEventListener('mousemove', function(e) {
     // Kiểm tra nếu đang trong trạng thái kéo
    if (isDragging) {
      e.preventDefault();// Ngăn chặn hành vi mặc định của trình duyệt khi kéo
      // Tính toán khoảng cách di chuyển của chuột
      currentX = initialX - e.clientX;
      currentY = initialY - e.clientY;
      initialX = e.clientX;
      initialY = e.clientY;
      // Di chuyển cửa sổ modal theo tọa độ chuột di chuyển
      addSkillModal.style.top = (addSkillModal.offsetTop - currentY) + 'px';
      addSkillModal.style.left = (addSkillModal.offsetLeft - currentX) + 'px';
    }
  });
// Xử lý sự kiện khi chuột được nhấc lên
  document.addEventListener('mouseup', function() {
     // Thiết lập trạng thái kéo về false khi kết thúc kéo
    isDragging = false;
    // Đổi con trỏ chuột về dạng mặc định
    addSkillModal.style.cursor = 'default';
  });
</script>




